alias zshconfig="vim ~/.zshrc"
alias zshcfg="zshconfig"
alias src="source ~/.zshrc"
alias editAliases="vim ~/.zsh_aliases"
alias ohmyzsh="vim ~/.oh-my-zsh"
alias vimconfig="vim ~/.vimrc"
alias vimcfg="vimconfig"
alias clr="clear"
alias home="cd ~ && clr"
alias apt="sudo apt-get"

# GIT
alias gs="git st"
alias gco="git co"
alias gci="git ci"
alias gl="git lg"
alias gb="git br"
alias gba="git br --all"
alias gdf="git df"
